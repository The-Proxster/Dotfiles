color-link default ""
color-link comment ", red"
color-link statement ", ${xrdb:color7:#222}"
color-link statusline "orange, blue"
color-link tabbar "blue, ${xrdb:color3:#222}"
color-link identifier "green, ${xrdb:color3:#222}"
color-link constant "orange, yellow"
color-link statement "red, blue"
color-link symbol "green, green"
